{
  "name": "@ax-llm/ax-examples",
  "version": "9.0.19",
  "type": "module",
  "description": "Docs for the best library to work with LLMs - Ax",
  "repository": {
    "type": "git",
    "url": "https://github.com/ax-llm/ax.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "node ../../scripts/clean.js build",
    "build:module": "tsc -p tsconfig.build.json",
    "watch:build": "tsc -p tsconfig.build.json -w",
    "tsx": "node --env-file=.env --import=tsx",
    "release": "release-it",
    "publish": "cd build && npm publish",
    "postbuild": "node ../../scripts/postbuild.js"
  },
  "dependencies": {
    "@ax-llm/ax": "9.0.19"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.1"
  },
  "files": [
    "build",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/@ax-llm/ax/issues"
  },
  "homepage": "https://github.com/@ax-llm/ax#readme",
  "author": "Vikram <https://twitter.com/dosco>"
}
