{
  "name": "@ax-llm/ax",
  "version": "9.0.19",
  "type": "module",
  "description": "The best library to work with LLMs",
  "typings": "./build/module/src/index.d.ts",
  "main": "./build/module/src/index.js",
  "module": "./build/module/src/index.js",
  "exports": "./build/module/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ax-llm/ax.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "build": "run-s build:*",
    "build:parser": "npx peggy --plugin ts-pegjs --extra-options-file ./dsp/parser.json --cache --output ./dsp/parser.ts ./dsp/parser.peggy",
    "build:module": "tsc -p ../../tsconfig.build.json",
    "watch:build": "tsc -p tsconfig.build.json -w",
    "tsx": "node --env-file=.env --import=tsx"
  },
  "engines": {
    "node": ">=21"
  },
  "dependencies": {
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "ts-pegjs": "^4.2.1",
    "tsx": "^4.7.1"
  },
  "files": [
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/@ax-llm/ax/issues"
  },
  "homepage": "https://github.com/@ax-llm/ax#readme",
  "author": "Vikram <https://twitter.com/dosco>"
}
