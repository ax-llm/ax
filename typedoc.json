{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-frontmatter",
    "./custom-frontmatter.mjs"
  ],
  "entryPoints": ["./src/ax/index.ts"],
  "out": "./src/docs/src/content/docs/apidocs",
  "pretty": true,
  "mergeReadme": true,
  "cleanOutputDir": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "readme": "README.md"
}
